<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="application">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.uibinder.UiBinder"/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>

    <source path="client"/>

    <set-configuration-property name="gin.ginjector.modules" value="com.nestlabs.demo.client.ClientModule"/>
    <set-configuration-property name="gwtp.bootstrapper" value="com.nestlabs.demo.client.Bootstrap"/>
</module>
